<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="java.com.lifeeditor.user_spec.model.user_specVO" table="user_spec" >
        
        <id name="userID" column="userID">
            <generator class="identity"/>
        </id>
        
        <property name="account" column="account" />
        <property name="pswd" column="pswd" />
        <property name="lastName" column="lastName" />
        <property name="firstName" column="firstName" />
        <property name="gender" column="gender" />
        <property name="birthdate" column="birthdate" />
        <property name="email" column="email" />
        <property name="address" column="address" />
        <property name="phone" column="phone" />
        <property name="genkiBarTol" column="genkiBarTol" /> 
        <property name="level" column="level" />
        <property name="picture" column="picture" />
        <property name="regTime" column="regTime" />
        <property name="hotMan" column="hotMan" />
        <property name="suspendType" column="suspendType" />        
               
    </class>
</hibernate-mapping>

